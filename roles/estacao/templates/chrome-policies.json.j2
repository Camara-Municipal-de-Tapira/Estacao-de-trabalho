{
  "ManagedBookmarks": [
    {% for bm in chrome_bookmarks %}
    { "toplevel_name": "{{ bm.name }}", "url": "{{ bm.url }}" }{% if not loop.last %},{% endif %}
    {% endfor %}
  ],
  "ExtensionInstallForcelist": [
    {% for ext in chrome_extensions %}
    "{{ ext }};https://clients2.google.com/service/update2/crx"{% if not loop.last %},{% endif %}
    {% endfor %}
  ]
}
